<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure> 
	<meta> 
    	<langDisable>1</langDisable> 
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<type>Array</type>
				 <TCEforms>
				 	<sheetTitle>Default</sheetTitle>
				</TCEforms>
				<el>
					<project_name>
						<TCEforms>
							<label>Project Name</label>
							<config>
								<type>input</type>
								<size>30</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</project_name>
					<project_manager>
						<TCEforms>
							<label>Project Manager</label>
							<config>
								<type>input</type>
								<size>30</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</project_manager>
				</el>	
			</ROOT>			
		</sDEF>
		<sContact>
			<ROOT>
				<type>Array</type>
				 <TCEforms>
				 	<sheetTitle>Contact</sheetTitle>
				</TCEforms>
				<el>
					<contacts>
						<TCEforms>
							<label>Contacts</label>
							<config>
								<type>select</type>
								<foreign_table>tt_address</foreign_table>
								<foreign_table_where></foreign_table_where>
								<size>5</size>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<default></default>
								<wizards>
									<add>
										<type>script</type>
										<title>new Contact</title>
										<script>wizard_add.php</script>
										<icon>add.gif</icon>
										<params>
											<table>tt_address</table>
											<pid>###CURRENT_PID###</pid>
											<setValue>prepend</setValue>
										</params>
									</add>
								</wizards>
							</config>
						</TCEforms>	
					</contacts>
				</el>
			</ROOT>
		</sContact>
		<sHosting>
			<ROOT>
				<type>Array</type>
				 <TCEforms>
				 	<sheetTitle>Hosting</sheetTitle>
				</TCEforms>
				<el>
					<hosting_provider>
						<TCEforms>
							<label>Provider</label>
							<config>
								<type>input</type>
								<size>30</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</hosting_provider>
					<hosting_account_id>
						<TCEforms>
							<label>Account ID</label>
							<config>
								<type>input</type>
								<size>30</size>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</hosting_account_id>
					<hosting_accounts>
						<TCEforms>
							<label>Accounts</label>
							<config>
								<type>select</type>
								<foreign_table>tx_caretaker_accounts</foreign_table>
								<foreign_table_where></foreign_table_where>
								<size>5</size>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<default></default>
								<wizards>
									<add>
										<type>script</type>
										<title>new Contact</title>
										<script>wizard_add.php</script>
										<icon>add.gif</icon>
										<params>
											<table>tx_caretaker_accounts</table>
											<pid>###CURRENT_PID###</pid>
											<setValue>prepend</setValue>
										</params>
									</add>
								</wizards>
							</config>
						</TCEforms>	
					</hosting_accounts>
				</el>
			</ROOT>
		</sHosting>
	</sheets>
</T3DataStructure> 


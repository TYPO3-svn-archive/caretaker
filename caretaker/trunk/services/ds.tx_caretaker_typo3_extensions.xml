<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<required_extensions>
				<TCEforms>
					<label><![CDATA[ Extensions:Versions wich are required ( Format: ExtKey [Operator] [Version] ) ]]></label>
					<config>
						<type>text</type>
						<cols>30</cols>
						<rows>5</rows>
					</config>
				</TCEforms>
			</required_extensions>	
			<forbidden_installed_extensions>
				<TCEforms>
					<label><![CDATA[ Extensions:Versions wich must not be installed ( Format: ExtKey [Operator] [Version] ) ]]></label>
					<config>
						<type>text</type>
						<cols>30</cols>
						<rows>5</rows>
					</config>
				</TCEforms>
			</forbidden_installed_extensions>
			<forbidden_existing_extensions>
				<TCEforms>
					<label><![CDATA[ Extensions:Versions wich must not exist ( Format: ExtKey [Operator] [Version] ) ]]></label>
					<config>
						<type>text</type>
						<cols>30</cols>
						<rows>5</rows>
					</config>
				</TCEforms>
			</forbidden_existing_extensions>
		</el>
	</ROOT>
</T3DataStructure>
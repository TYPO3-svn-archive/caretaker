						</tx_templavoila>
						<el>
						
							<test_service>
								<TCEforms>
									<exclude>1</exclude>
									<label>LLL:EXT:caretaker/locallang_db.xml:tx_caretaker_test.test_service</label>
									<config>
										<type>select</type>
										<items>
											<numIndex index="0">

<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
			<ROOT>
				<TCEforms> 
                        <sheetTitle>Default</sheetTitle> 
                </TCEforms> 
				<type>array</type>
				<el>
					<selenium_configuration>
						<TCEforms>
							<label>Selenese Configuration</label>
							<config>
								<type>text</type>
								<rows>10</rows>
							</config>
						</TCEforms>
					</selenium_configuration>
					
					<response_time_error>
						<TCEforms>
							<label>Maximal allowed Response Time in Seconds</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>10</size>
							</config>
						</TCEforms>
					</response_time_error>
					
					<response_time_warning>
						<TCEforms>
							<label>Warning Response Time in Seconds</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
							</config>
						</TCEforms>
					</response_time_warning>

					<selenium_server>
						<TCEforms>
							<label>Select Selenium Server</label>
							<config>
								<type>select</type>
								<foreign_table>tx_caretakerselenium_server</foreign_table>
								<!-- 
								<items> 
									<numIndex index="0"> 
										<numIndex index="0">All Servers</numIndex> 
										<numIndex index="1">-1</numIndex> 
									</numIndex> 
                                </items> 
                                -->
								<maxitems>9</maxitems>
								<minitems>1</minitems>
								<size>5</size>
								<default>0</default>
							</config>
						</TCEforms>
					</selenium_server>
					
				</el>
			</ROOT>

</T3DataStructure>
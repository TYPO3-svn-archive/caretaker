<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sSYSTEM>
			<ROOT>
				<TCEforms> 
                        <sheetTitle>OS</sheetTitle> 
                </TCEforms> 
				<type>array</type>
				<el>
					<min_os_version>
						<TCEforms>
							<label>minimal OS Version</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</min_os_version>
					<max_os_version>
						<TCEforms>
							<label>maximal OS Version</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</max_os_version>
					<installed_tools>
						<TCEforms>
							<label>installed Tools (Format: Key:[MinVersion]:[MaxVersion])</label>
							<config>
								<type>text</type>
							</config>
						</TCEforms>
					</installed_tools>						
	
				</el>
			</ROOT>
		</sSYSTEM>
		<sAPACHE>
			<ROOT>
				<TCEforms> 
                        <sheetTitle>Apache</sheetTitle> 
                </TCEforms> 
				<type>array</type>
				<el>
					<min_apach_version>
						<TCEforms>
							<label>minimal Apache-Version</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</min_apach_version>
					<max_apache_version>
						<TCEforms>
							<label>maximal Apache-Version</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</max_apache_version>
					<apache_modules>
						<TCEforms>
							<label>Apache-Modules (Format: Key:[MinVersion]:[MaxVersion])</label>
							<config>
								<type>text</type>
							</config>
						</TCEforms>
					</apache_modules>
					<apache_ini_values>
						<TCEforms>
							<label>Apach Config-Values (Format: Key:[MinVersion]:[MaxVersion], Key=[Value])</label>
							<config>
								<type>text</type>
							</config>
						</TCEforms>
					</apache_ini_values>
				</el>
			</ROOT>
		</sAPACHE>
		<sPHP>
			<ROOT>
				<TCEforms> 
                        <sheetTitle>PHP</sheetTitle> 
                </TCEforms> 
				<type>array</type>
				<el>
					<min_php_version>
						<TCEforms>
							<label>minimal PHP-Version</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</min_php_version>
					<max_php_version>
						<TCEforms>
							<label>maximal PHP-Version</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</max_php_version>
					<php_modules>
						<TCEforms>
							<label>PHP-Modules (Format: Key:[MinVersion]:[MaxVersion])</label>
							<config>
								<type>text</type>
							</config>
						</TCEforms>
					</php_modules>
					<php_ini_values>
						<TCEforms>
							<label>PHP.ini Values (Format: Key:[MinVersion]:[MaxVersion], Key=[Value])</label>
							<config>
								<type>text</type>
							</config>
						</TCEforms>
					</php_ini_values>
				</el>
			</ROOT>
		</sPHP>
		<sMYSQL>
			<ROOT>
				<TCEforms> 
                        <sheetTitle>mySQL</sheetTitle> 
                </TCEforms> 
				<type>array</type>
				<el>
					<min_mysql_version>
						<TCEforms>
							<label>minimal mySQL Vesion</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</min_mysql_version>
					<max_mysql_version>
						<TCEforms>
							<label>maximal mySQL Vesion</label>
							<config>
								<type>input</type>
								<size>10</size>
							</config>
						</TCEforms>
					</max_mysql_version>
					<mysql_check>
						<TCEforms>
							<label>check mySQL DM</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</mysql_check>
				</el>
			</ROOT>
		</sMYSQL>
	</sheets>
</T3DataStructure>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms> 
                      <sheetTitle>HTTP-Communication</sheetTitle> 
              </TCEforms> 
		<type>array</type>
		<el>
		
			<snmp_description>
				<TCEforms>
					<label>SNMP-Value-Description</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</snmp_description>
			
			<snmp_community>
				<TCEforms>
					<label>SNMP-Comnunity</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</snmp_community>	
								
			<snmp_object_id>
				<TCEforms>
					<label>SNMP-ObjectID</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</snmp_object_id>	

			<snmp_timeout>
				<TCEforms>
					<label>SNMP-Timeout (nonoseconds)</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</snmp_timeout>	

			<snmp_retries>
				<TCEforms>
					<label>SNMP-Retries</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</snmp_retries>	
														
			<value_range_error>
				<TCEforms>
					<label>Value Range for Errors</label>
					<config>
						<type>input</type>
						<size>10</size>
					</config>
				</TCEforms>
			</value_range_error>
			
			<value_range_warning>
				<TCEforms>
					<label>Value Range for Warnings</label>
					<config>
						<type>input</type>
						<size>10</size>
					</config>
				</TCEforms>
			</value_range_warning>

		</el>
	</ROOT>
</T3DataStructure>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms>
			<sheetTitle>HTTP-Communication</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
		
			<rsml_url>
				<TCEforms>
					<label>Remote Script URL  ATENTION: Security is your Problem so please be carefull </label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</rsml_url>
			
			<rsml_id>
				<TCEforms>
					<label>Remote Script ID</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</rsml_id>

			<rsml_version>
				<TCEforms>
					<label>Remote Script Version (&lt; and &gt; are allowed) </label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</rsml_version>
								
			<expected_status>
				<TCEforms>
					<label>Expected Status of Remote Script.</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</expected_status>

			<expected_value>
				<TCEforms>
					<label>Expected Value of Remote Script.</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</expected_value>
			
		</el>
	</ROOT>
</T3DataStructure>